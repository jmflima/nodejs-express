<h1>Todos os produtos</h1>
<div class="products">
  {{#each products}}
    <div class="product">
      <img src="{{this.imagem}}" alt="{{this.nome}}">
      <h3>{{this.nome}}</h3>
      <p>R${{this.preco}}</p>
      <div class="actions">
        <a href="/products/{{this._id}}">Detalhes</a>
      </div>
    </div>
  {{/each}}
</div>